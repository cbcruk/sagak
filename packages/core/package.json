{
  "name": "sagak-core",
  "version": "0.1.0",
  "description": "Core library for Sagak Editor - a modern WYSIWYG editor",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "rich-text",
    "contenteditable",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/sagak-editor.git",
    "directory": "packages/core"
  },
  "devDependencies": {
    "tsup": "^8.4.0"
  }
}
